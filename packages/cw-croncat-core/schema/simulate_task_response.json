{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "SimulateTaskResponse",
  "type": "object",
  "required": [
    "estimated_gas",
    "occurrences",
    "task_hash"
  ],
  "properties": {
    "estimated_gas": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "occurrences": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "task_hash": {
      "type": "string"
    }
  }
}
